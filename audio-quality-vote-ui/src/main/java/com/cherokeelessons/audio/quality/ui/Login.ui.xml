<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialContainer ui:field="container"
			containerEnabled="true">
			<m:MaterialRow>
				<m:MaterialColumn grid="s1 m2 l3" hideOn="NONE">
					<m:html.Span style="color: white; width: 0px;">.</m:html.Span>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s12 m8 l6">
					<m:MaterialPanel>
						<m:MaterialContainer>
							<m:MaterialButton ui:field="btnLogin"
								text="Login" />
						</m:MaterialContainer>
					</m:MaterialPanel>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1 m2 l3"
					hideOn="HIDE_ON_SMALL_DOWN">
					<m:html.Span style="color: white; width: 0px;">.</m:html.Span>
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialContainer>
	</m:MaterialPanel>
</ui:UiBinder> 